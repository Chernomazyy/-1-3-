using System;
using System.Collections.Generic;
namespace studyProject
{
	class programm
	{
		static void Main(string[] args)
		{
			int a = Convert.ToInt32(Console.ReadLine());
			int b = Convert.ToInt32(Console.ReadLine());
			int c = Convert.ToInt32(Console.ReadLine());
			a = a >= 1 && a <= 3 ?a:404;
			b = b >= 1 && b <= 3 ? b : 404;
			c = c >= 1 && c <= 3 ? c : 404;
			Console.WriteLine(a);
			Console.WriteLine(b);
			Console.WriteLine(c);
		}
	}
}
